{
  "name": "tecnobot-saas",
  "version": "2.0.0",
  "description": "Multi-tenant WhatsApp chatbot SAAS platform with AI capabilities",
  "main": "app.js",
  "scripts": {
    "start": "node src/app.js",
    "dev": "node scripts/dev.js",
    "dev:simple": "nodemon src/app.js",
    "init": "node scripts/init-project.js",
    "setup": "node scripts/setup.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:integration": "jest --testPathPattern=tests/integration",
    "lint": "eslint src/ --ext .js",
    "lint:fix": "eslint src/ --ext .js --fix",
    "format": "prettier --write src/**/*.js",
    "db:migrate": "node scripts/migrate.js",
    "db:migrate:status": "node scripts/migrate.js status",
    "db:seed": "node scripts/seed.js",
    "db:reset": "node scripts/reset.js",
    "build": "npm run lint && npm run test",
    "docker:build": "docker build -t tecnobot-saas .",
    "docker:run": "docker run -p 3020:3020 --env-file .env tecnobot-saas",
    "docker:dev": "docker-compose up --build",
    "logs": "tail -f logs/combined.log",
    "logs:error": "tail -f logs/error.log",
    "health": "curl -f http://localhost:3020/health || exit 1",
    "precommit": "npm run lint && npm run test",
    "postinstall": "node scripts/setup.js",
    "crud": "node pruebaCrud/server.js"
  },
  "keywords": [
    "whatsapp",
    "chatbot",
    "ai",
    "supabase",
    "saas",
    "multi-tenant",
    "baileys",
    "openai",
    "express",
    "jwt"
  ],
  "author": "TecnoBot Team",
  "license": "MIT",
  "dependencies": {
    "@builderbot/bot": "^1.0.0",
    "@builderbot/provider-baileys": "^1.0.0",
    "@google-cloud/text-to-speech": "^5.0.1",
    "@supabase/supabase-js": "^2.39.3",
    "@whiskeysockets/baileys": "^6.5.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^2.0.6",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "node-cache": "^5.1.2",
    "node-cron": "^3.0.3",
    "nodemailer": "^6.9.7",
    "openai": "^4.86.1",
    "qrcode": "^1.5.3",
    "sharp": "^0.32.6",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "twilio": "^4.19.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.4",
    "chokidar": "^3.6.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "jest": "^29.7.0",
    "nock": "^13.4.0",
    "nodemon": "^3.0.3",
    "prettier": "^3.1.0",
    "sinon": "^17.0.1",
    "supertest": "^6.3.3"
  },
  "optionalDependencies": {
    "sharp-win32-x64": "^0.33.5"
  }
}
